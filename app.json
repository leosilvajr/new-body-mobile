{
  "expo": {
    "name": "New Body",
    "slug": "new-body",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e2ba3"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.seuusuario.newbody",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#1e2ba3"
      }
    },
    "web": {
      "favicon": "./src/assets/logo.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "77a9f066-6c2f-4675-b8b2-d285f907f410"
      }
    }
  }
}
